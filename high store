<!-- โ ุฅุดุนุงุฑ ุนุงุฆู -->
<div id="toast" style="display:none; position: fixed; bottom: 100px; left: 50%; transform: translateX(-50%); background: #f3d76e; color: #121212; padding: 10px 20px; border-radius: 30px; font-weight: bold; z-index: 9999;">ุชูุช ุงูุฅุถุงูุฉ ููุณูุฉ โ</div>

<script>
let cart = [];

function showToast(msg) {
  const toast = document.getElementById("toast");
  toast.textContent = msg;
  toast.style.display = "block";
  setTimeout(() => { toast.style.display = "none"; }, 3000);
}

function addToCart(product) {
  cart.push(product);
  document.getElementById("cart-count").textContent = cart.length;
  document.getElementById("addSound").play();
  showToast(`โ ุชูุช ุฅุถุงูุฉ ${product.name} ููุณูุฉ`);
  renderCart();
}

function renderCart() {
  const container = document.getElementById("cart-items");
  container.innerHTML = '';
  let total = 0;
  cart.forEach((item, index) => {
    total += item.price;
    const div = document.createElement("div");
    div.innerHTML = `<strong>${item.name}</strong> - ${item.price} ุฌููู <button onclick="removeItem(${index})">โ</button>`;
    container.appendChild(div);
  });
  document.getElementById("cart-total").textContent = total;
}

function removeItem(index) {
  cart.splice(index, 1);
  document.getElementById("cart-count").textContent = cart.length;
  renderCart();
}

document.querySelectorAll(".btn-add-cart").forEach(btn => {
  btn.addEventListener("click", () => {
    const card = btn.closest(".product-card");
    const name = card.querySelector(".product-name").textContent;
    const desc = card.querySelector(".product-desc").textContent;
    const price = parseInt(card.querySelector(".product-price").textContent);
    addToCart({ name, desc, price });
  });
});

document.getElementById("cart-btn").onclick = () => {
  document.getElementById("cart-drawer").style.display = "block";
  renderCart();
};
document.getElementById("close-cart").onclick = () => {
  document.getElementById("cart-drawer").style.display = "none";
};

document.getElementById("checkout-btn").onclick = () => {
  document.getElementById("order-confirmation").style.display = "block";
};
document.getElementById("cancel-order").onclick = () => {
  document.getElementById("order-confirmation").style.display = "none";
};

document.getElementById("order-form").onsubmit = function (e) {
  e.preventDefault();
  const name = this['customer-name'].value;
  const address = this['customer-address'].value;
  const phone = this['customer-phone1'].value;

  let message = `๐ฆ ุทูุจ ุฌุฏูุฏ ูู HIGH STORE:\n\n๐ค ุงูุงุณู: ${name}\n๐ ุงูุนููุงู: ${address}\n๐ ุฑูู ุงููุงุชู: ${phone}\n\n๐๏ธ ุงูููุชุฌุงุช ุงููุทููุจุฉ:\n`;
  let total = 0;
  cart.forEach((item, i) => {
    message += `${i + 1}. ${item.name} - ${item.desc} - ุงูุณุนุฑ: ${item.price} ุฌููู\n`;
    total += item.price;
  });
  message += `\n๐ฐ ุงูุฅุฌูุงูู: ${total} ุฌููู\n\nโ HIGH STORE | ุดูุฑุงู ูุชุณููู ูุนูุง ๐ค`;

  const url = `https://wa.me/201055815900?text=${encodeURIComponent(message)}`;
  window.open(url, '_blank');
  cart = [];
  document.getElementById("cart-count").textContent = 0;
  renderCart();
  document.getElementById("order-confirmation").style.display = "none";
  document.getElementById("cart-drawer").style.display = "none";
};

document.getElementById("searchInput").addEventListener("input", filter);
document.getElementById("categoryFilter").addEventListener("change", filter);

function filter() {
  const term = document.getElementById("searchInput").value.toLowerCase();
  const category = document.getElementById("categoryFilter").value;
  document.querySelectorAll("section[data-category]").forEach(sec => {
    const secCat = sec.getAttribute("data-category");
    let show = false;
    sec.querySelectorAll(".product-card").forEach(card => {
      const text = card.textContent.toLowerCase();
      const match = text.includes(term) && (!category || secCat === category);
      card.style.display = match ? "block" : "none";
      if (match) show = true;
    });
    sec.style.display = show ? "block" : "none";
  });
}

document.getElementById("priceSort").addEventListener("change", () => {
  const sortType = document.getElementById("priceSort").value;
  document.querySelectorAll(".products").forEach(grid => {
    const items = Array.from(grid.children);
    items.sort((a, b) => {
      const priceA = parseFloat(a.querySelector(".product-price").textContent);
      const priceB = parseFloat(b.querySelector(".product-price").textContent);
      return sortType === "low" ? priceA - priceB : priceB - priceA;
    });
    grid.innerHTML = "";
    items.forEach(item => grid.appendChild(item));
  });
});
</script>

